config_version=4

[MonoCustomResourceRegistry]

ClassPrefix=""
SearchType=0
ResourceScriptDirectories=[ "res://" ]

[application]

config/name="Our Ruinous Gods"
run/main_scene="res://menus/StartMenu.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout="res://audio_mixer.tres"

[autoload]

ResourceDatabase="*res://resources/ResourceDatabase.tscn"
EventBus="*res://scripts/EventBus.cs"
AudioSettings="*res://resources/AudioMixer.tscn"
GlobalAudioStreamPlayer="*res://resources/AudioStreamPlayer.tscn"
InputBindings="*res://resources/InputBindings.tscn"
InputLayoutTracker="*res://resources/InputLayoutTracker.tscn"
VersionInfo="*res://scripts/VersionInfo.cs"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Godot-Mono-CustomResourceRegistry-godot-3.x/addons/MonoCustomResourceRegistry/plugin.cfg" )

[global]

rendering=false
theme=false
text=false

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://themes/DefaultTheme.tres"

